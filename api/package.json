{
  "name": "rest-api",
  "version": "0.0.1",
  "description": "RestApi",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "20 || 22 || 24"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "pnpm run eslint && pnpm run prettier:check",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "pnpm run prettier:cli -- -l",
    "prettier:fix": "pnpm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "pnpm run eslint -- --fix",
    "pretest": "pnpm run rebuild",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "posttest": "pnpm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && pnpm run posttest",
    "docker:build": "docker build -t rest-api .",
    "docker:run": "docker run -p 3000:3000 -d rest-api",
    "premigrate": "pnpm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "pnpm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "pnpm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "pnpm run clean && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Aron0228 <aronreszegi2@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^8.0.5",
    "@loopback/core": "^7.0.4",
    "@loopback/repository": "^8.0.4",
    "@loopback/rest": "^15.0.5",
    "@loopback/rest-explorer": "^8.0.5",
    "@loopback/service-proxy": "^8.0.4",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^12.0.4",
    "@loopback/eslint-config": "^16.0.1",
    "@loopback/testlab": "^8.0.4",
    "@types/node": "^16.18.126",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.57.1",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2",
    "vitest": "^4.0.18"
  }
}